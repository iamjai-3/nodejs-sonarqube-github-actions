{
  "name": "nodejs-sonarqube-github-actions",
  "version": "0.0.0",
  "description": "TypeScript + TypeORM + PostgreSQL + Express API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "sonar": "./node_modules/sonar-scanner/bin/sonar-scanner",
    "lint:fix": "npm run lint -- --fix",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "npm run typeorm schema:sync -- -d src/data-source.ts",
    "schema:drop": "npm run typeorm schema:drop -- -d src/data-source.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/data-source.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d src/data-source.ts ./src/database/migrations/%npm_config_filename%",
    "migration:run": "npm run typeorm migration:run -- -d src/data-source.ts",
    "migration:drop": "npm run typeorm migration:revert -- -d src/data-source.ts",
    "migration:create": "npm run typeorm migration:create src/database/migrations/%npm_config_filename%"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typedi": "^0.10.0",
    "typeorm": "0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.93",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.3",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.19",
    "@types/hpp": "^0.2.3",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.8.6",
    "@types/supertest": "^2.0.14",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.1",
    "node-config": "^0.0.2",
    "node-gyp": "^9.4.0",
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0",
    "prettier": "^3.0.3",
    "sonar-scanner": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.2.2"
  }
}
